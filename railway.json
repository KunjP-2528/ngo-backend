{
  "builds": [
    {
      "src": "package.json",
      "use": "@railway/node",
      "config": {
        "installCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "continue": true
    }
  ]
}